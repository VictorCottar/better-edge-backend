datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Cliente {
  id       Int     @id @default(autoincrement())
  nome     String
  email    String  @unique
  status   Boolean
  ativos   Ativos[]
}

model Ativos {
  id          Int     @id @default(autoincrement())
  nome        String
  valorAtual  Float
  clienteId   Int
  cliente     Cliente @relation(fields: [clienteId], references: [id])
}
